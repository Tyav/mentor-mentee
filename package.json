{
  "name": "mentor-app-backend",
  "version": "1.0.0",
  "description": "a mentor-mentee connect app.",
  "main": "index.js",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.13.0"
  },
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "start:debug": "cross-env DEBUG=express-mongoose-es6-rest-api:* yarn start",
    "lint": "esw *.js config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test && jest --runInBand --verbose --detectOpenHandles --forceExit",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test --coverage",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tyav/mentor-mentee.git"
  },
  "keywords": [
    "mentor",
    "mentee",
    "connect",
    "decagon"
  ],
  "author": "decagon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tyav/mentor-mentee/issues"
  },
  "homepage": "https://github.com/Tyav/mentor-mentee#readme",
  "dependencies": {
    "@types/jest": "^24.0.17",
    "aws-sdk": "^2.505.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "celebrate": "^10.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-winston": "^3.2.1",
    "gravatar": "^1.8.0",
    "helmet": "^3.20.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.26",
    "mongoose": "^5.6.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multer-sharp": "^0.6.1",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "otp-generator": "^1.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.26.1",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-watch": "^5.1.2",
    "husky": "^2.4.1",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  }
}
